public class Main
{
  public static void main(String args[])
  {
      bubble list = new bubble();
      
      list.randomizeList();
      System.out.println("Randomized List: ");
      list.print();
      list.bubbleSort();
      System.out.println("Sorted list: ");
      list.print();
      
      System.out.println("\n Making new list to bubble sort \n");
      
      list.randomizeList();
      System.out.println("Randomized List: ");
      list.print();
      list.bubbleSort();
      System.out.println("Sorted list: ");
      list.print();       
  }  
}
